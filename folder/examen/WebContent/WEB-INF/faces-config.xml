<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
	<managed-bean>
		<managed-bean-name>marquesBackingBean</managed-bean-name>
		<managed-bean-class>backingBean.MarquesBackingBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>vehiculesBackingBean</managed-bean-name>
		<managed-bean-class>backingBean.VehiculesBackingBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>modifierVehiculeBackingBean</managed-bean-name>
		<managed-bean-class>backingBean.ModifierVehiculeBackingBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>nouveauVehiculeBackingBean</managed-bean-name>
		<managed-bean-class>backingBean.NouveauVehiculeBackingBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Alle</from-outcome>
			<to-view-id>/marques.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>marques.xhtml</display-name>
		<from-view-id>/marques.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>formModifier</from-outcome>
			<to-view-id>/modifier.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>modifier.xhtml</display-name>
		<from-view-id>/modifier.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>vehiculeModifierAvecSucces</from-outcome>
			<to-view-id>/marques.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>marques.xhtml</display-name>
		<from-view-id>/marques.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>formAdjouter</from-outcome>
			<to-view-id>/adjouterVehicule.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>adjouterVehicule.xhtml</display-name>
		<from-view-id>/adjouterVehicule.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>NouveauVehiculeSauvegarde</from-outcome>
			<to-view-id>/marques.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
